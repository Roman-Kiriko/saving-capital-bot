{
  "name": "saving-capital-bot",
  "version": "0.0.1",
  "description": "Telegraf TS bot",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "dev": "ts-node-dev -r dotenv/config src/",
    "dev:task": "ts-node-dev -r dotenv/config src/tasks/",
    "task": "node dist/tasks/",
    "start": "node dist/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "telegraf",
    "typescript"
  ],
  "author": "Roman Vilkov <aurimcreo@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/Roman-Kiriko/saving-capital-bot#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "telegraf": "^4.3.0",
    "upgrade": "^1.1.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@types/node-fetch": "^2.5.8",
    "dotenv": "^8.2.0",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  }
}
